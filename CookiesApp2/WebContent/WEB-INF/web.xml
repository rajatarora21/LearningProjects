<?xml version="1.0" encoding="UTF-8"?>
<web-app>
	<welcome-file-list>
	<welcome-file>input.html</welcome-file>
	</welcome-file-list>
	<servlet>
		<servlet-name>App1</servlet-name>
		<servlet-class>edu.aptech.servlet.SetCookiesServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>App1</servlet-name>
		<url-pattern>/url1</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>App2</servlet-name>
		<servlet-class>edu.aptech.servlet.GetCookiesServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>App2</servlet-name>
		<url-pattern>/url2</url-pattern>
	</servlet-mapping>
</web-app>